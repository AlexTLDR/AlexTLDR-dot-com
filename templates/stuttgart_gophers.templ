package templates

templ StuttgartGophers() {
	<!-- Stuttgart Gophers Section -->
	<section id="stuttgart-gophers" class="min-h-screen bg-base-200">
		<div class="container mx-auto px-8 py-16">
			<div class="text-center mb-16">
				<h2 class="text-4xl font-bold text-primary mb-4">Stuttgart Gophers</h2>
				<p class="text-lg">Leading the local Go community</p>
			</div>
			<div class="hero bg-base-100 rounded-lg">
				<div class="hero-content text-center">
					<div class="max-w-3xl">
						<h3 class="text-2xl font-bold mb-4">🐹 Go Community in Stuttgart</h3>
						<p class="text-lg mb-6">
							Information about the Stuttgart Gophers meetup, upcoming events, and community activities will go here...
						</p>
						<div class="flex justify-center space-x-4">
							<button class="btn btn-primary">Join Meetup</button>
							<button class="btn btn-outline">View Events</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
}
